<% layout('layouts/boilerplate')%>
<div class="table-responsive-sm">
    <table class="table table-bordered border-danger align-bottom">
        <thead>
            <tr>
                <th scope="col">L/M</th>
                <th scope="col">Jan</th>
                <th scope="col">Feb</th>
                <th scope="col">Mar</th>
                <th scope="col">Apr</th>
                <th scope="col">May</th>
                <th scope="col">Jun</th>
                <th scope="col">Jul</th>
                <th scope="col">Avg</th>
                <th scope="col">Sep</th>
                <th scope="col">Okt</th>
                <th scope="col">Nov</th>
                <th scope="col">Dec</th>
                <th scope="col">Kat.</th>
                <th scope="col">SUM</th>
            </tr>
        </thead>
        <tbody>
            <% for (const year of context) { %>
                <tr>
                    <th scope="row"><%= year[0] %></th>
                    <td scope="col"><%= year[1].Jan ? year[1].Jan.sum.toFixed(0) : "0" %></td>
                    <td scope="col"><%= year[1].Feb ? year[1].Feb.sum.toFixed(0) : "0" %></td>
                    <td scope="col"><%= year[1].Mar ? year[1].Mar.sum.toFixed(0) : "0" %></td>
                    <td scope="col"><%= year[1].Apr ? year[1].Apr.sum.toFixed(0) : "0" %></td>
                    <td scope="col"><%= year[1].May ? year[1].May.sum.toFixed(0) : "0" %></td>
                    <td scope="col"><%= year[1].Jun ? year[1].Jun.sum.toFixed(0) : "0" %></td>
                    <td scope="col"><%= year[1].Jul ? year[1].Jul.sum.toFixed(0) : "0" %></td>
                    <td scope="col"><%= year[1].Avg ? year[1].Avg.sum.toFixed(0) : "0" %></td>
                    <td scope="col"><%= year[1].Sep ? year[1].Sep.sum.toFixed(0) : "0" %></td>
                    <td scope="col"><%= year[1].Oct ? year[1].Oct.sum.toFixed(0) : "0" %></td>
                    <td scope="col"><%= year[1].Nov ? year[1].Nov.sum.toFixed(0) : "0" %></td>
                    <td scope="col"><%= year[1].Dec ? year[1].Dec.sum.toFixed(0) : "0" %></td>
                    <td>XXX</td> 
                    <td scope="col"><%= year[1] ? year[1].sum.toFixed(0) : "0" %></td>
                </tr>
            <% } %>
        </tbody>
    </table>
</div>
